<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="ru.melandra.react">

    <uses-permission android:name="android.permission.INTERNET" />
    <uses-permission android:name="android.permission.ACCESS_NETWORK_STATE" />

    <application
        android:name=".Leak.LeakApp"
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme"
        android:usesCleartextTraffic="true">
        <activity android:name=".Leak.LeakActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".Test.TestActivity">
        </activity>
        <activity android:name=".DI.Dagger.DaggerActivity"></activity>
        <activity android:name=".DI.Di.DiActivity" />
        <activity android:name=".DI.NoDi.NoDiActivity" />
        <activity android:name=".Room.RoomActivity" />
        <activity android:name=".Retrofit.RetrofitActivity" />
        <activity android:name=".GsonActivity" />
        <activity android:name=".RxSingleActivity" />
        <activity android:name=".RxActivity" />
    </application>

</manifest>